<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/dinnerImageView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="16dp"
        android:contentDescription="Image of a dinner steak"
        android:scaleType="centerCrop"
        android:src="@drawable/_image07"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dinnerTextView"
        tools:ignore="HardcodedText" />

    <ImageView
        android:id="@+id/dinnerImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dinnerTextView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:contentDescription="Image of a dinner steak"
        android:scaleType="centerCrop"
        android:src="@drawable/_image08"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="235dp"
        tools:ignore="DuplicateIds,HardcodedText" />

    <ImageView
        android:id="@+id/dinnerImageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dinnerTextView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:contentDescription="Image of a dinner steak"
        android:scaleType="centerCrop"
        android:src="@drawable/_image09"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="468dp"
        tools:ignore="DuplicateIds,HardcodedText,UnknownId" />


</androidx.constraintlayout.widget.ConstraintLayout>